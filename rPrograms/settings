f = fs.open("rSettings/useAdmin", "r")
useAdmin = f.readLine()
f.close()

term.setTextColor(colors.white)
term.setBackgroundColor(colors.cyan)
print("Settings|")
print("---------")
term.setCursorPos(51, 1)
print("X")

if useAdmin == "true" then
  term.setCursorPos(1,4)
  write("Use Admin [X]")
else
  term.setCursorPos(1,4)
  write("Use Admin [ ]")
end

